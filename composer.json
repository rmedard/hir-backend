{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "moment": {
            "type": "package",
            "package": {
                "name": "moment/moment",
                "version": "2.29.1",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/moment/moment",
                    "reference": "2.29.1"
                }
            }
        },
        "moment-tz": {
            "type": "package",
            "package": {
                "name": "moment/moment-timezone",
                "version": "0.5.34",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/moment/moment-timezone/",
                    "reference": "0.5.34"
                }
            }
        },
        "jQuery.countdown": {
            "type": "package",
            "package": {
                "name": "jquery/jquery-countdown",
                "version": "2.2.0",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/hilios/jQuery.countdown",
                    "reference": "v2.2.0"
                }
            }
        },
        "flexslider": {
            "type": "package",
            "package": {
                "name": "flexslider/flexslider",
                "version": "2.7.2",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/woocommerce/FlexSlider",
                    "reference": "2.7.2"
                }
            }
        },
        "colorbox": {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/jackmoore/colorbox",
                    "reference": "1.6.4"
                }
            }
        },
        "fontawesome": {
            "type": "package",
            "package": {
                "name": "fontawesome/fontawesome",
                "version": "7.0.1",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/FortAwesome/Font-Awesome",
                    "reference": "7.0.1"
                }
            }
        },
        "jquery.number": {
            "type": "package",
            "package": {
                "name": "jquery/jquery-number",
                "version": "2.1.6",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/customd/jquery-number",
                    "reference": "2.1.6"
                }
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "composer/installers": "^1.9",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/adsense": "^1.2",
        "drupal/back_to_top": "^3.0",
        "drupal/better_exposed_filters": "^7.1@beta",
        "drupal/block_content_permissions": "^1.6",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/bpmn_io": "^2.0",
        "drupal/clientside_validation": "^4.0",
        "drupal/colorbox": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10.3",
        "drupal/csv_serialization": "^4.0",
        "drupal/devel": "^5.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/eca": "^2.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/field_group": "^4.0",
        "drupal/field_permissions": "^1.2",
        "drupal/flexslider": "^3.0@alpha",
        "drupal/fontawesome": "^3.0",
        "drupal/geolocation": "^4.0@beta",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.0",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_checkboxradio": "^2.0",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_effects": "^2.0",
        "drupal/jquery_ui_slider": "^2.0",
        "drupal/jquery_ui_tabs": "^2.0",
        "drupal/jquery_ui_tooltip": "^2.0",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/jsonapi_extras": "^3.20",
        "drupal/mailer_transport": "^1.6",
        "drupal/mailsystem": "^4.5",
        "drupal/memcache": "^2.3",
        "drupal/metatag": "^2.0",
        "drupal/module_filter": "^5.0",
        "drupal/pathauto": "^1.8",
        "drupal/plugin": "^2.9",
        "drupal/publication_date": "^3.0",
        "drupal/queue_ui": "^3.1",
        "drupal/recaptcha": "^3.0",
        "drupal/restui": "^1.16",
        "drupal/shs": "^3.0@beta",
        "drupal/single_content_sync": "^1.4",
        "drupal/smart_ip": "^5.0@alpha",
        "drupal/smtp": "^1.4",
        "drupal/telephone_validation": "^2.2",
        "drupal/token": "^1.5",
        "drupal/twig_extensions": "^2.5",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/upgrade_status": "^4.0",
        "drupal/views_autocomplete_filters": "^2.0",
        "drupal/views_bootstrap": "^5.5@RC",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.4",
        "drupal/views_list_sort": "^1.4",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^6.3@beta",
        "drupal/webform_rest": "^4.2",
        "drupal/webform_views": "^5.2",
        "drupal/youtube": "^3.0@beta",
        "drush/drush": "^13",
        "flexslider/flexslider": "^2.7",
        "fontawesome/fontawesome": "^7.0",
        "jackmoore/colorbox": "^1.6",
        "jquery/jquery-countdown": "^2.2",
        "jquery/jquery-number": "^2.1",
        "moment/moment": "^2.29",
        "moment/moment-timezone": "^0.5.34"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": ".editorconfig",
                ".gitattributes": ".gitattributes"
            },
            "locations": {
                "web-root": "httpdocs/"
            }
        },
        "installer-paths": {
            "httpdocs/core": [
                "type:drupal-core"
            ],
            "httpdocs/libraries/{$name}": [
                "type:drupal-library"
            ],
            "httpdocs/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "httpdocs/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "httpdocs/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "httpdocs/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "httpdocs/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
